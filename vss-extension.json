{
  "manifestVersion": 1,
  "id": "team-member-randomizer",
  "version": "0.0.79",
  "name": "Team Member Randomizer",
  "description": "Team Member Randomizer for Daily Standups",
  "publisher": "enginpolat",
  "public": false,
  "categories": [
    "Azure Boards"
  ],
  "links": {
    "learn": {
      "uri": "https://github.com/polatengin/tokyo"
    }
  },
  "icons": {
    "default": "src/icon.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/2.0"
  ],
  "content": {
    "details": {
      "path": "DETAILS.md"
    }
  },
  "branding": {
    "color": "rgb(190, 39, 3)",
    "theme": "dark"
  },
  "files": [
    {
      "path": "src/icon.png",
      "addressable": true
    },
    {
      "path": "src/main.html",
      "addressable": true
    },
    {
      "path": "src/popupDialog.html",
      "addressable": true
    },
  ]
}
